{
  "id": "netlify-button",
  "name": "Netlify Build",
  "src": "http://localhost:1234",
  "sidebar": true,
  "parameters": {
    "installation": [
      {
        "id": "publishKey",
        "type": "Symbol",
        "name": "PubNub publish key",
        "description": "A public PubNub publish key to emit update events",
        "required": true
      },
      {
        "id": "subscribeKey",
        "type": "Symbol",
        "name": "PubNub subscribe key",
        "description": "A public PubNub subscribe key to listen for update events",
        "required": true
      },
      {
        "id": "channels",
        "type": "Symbol",
        "name": "Comma seperated Netlify channel list (max 3)",
        "description": "Example: my-app-abc,my-second-app-def,my-third-app-ghi",
        "required": true
      },
      {
        "id": "netlifySiteIds",
        "type": "Symbol",
        "name": "Comma seperated Netlify Site Id list (max 3)",
        "description": "Example: 14234e3,143248,35233",
        "required": true
      },
      {
        "id": "buildHookUrls",
        "type": "Symbol",
        "name": "Comma seperated Netlify build hook URLs (max 3)",
        "description": "Example: https://api.netlify.com/build_hooks/xyz,https://api.netlify.com/build_hooks/abc",
        "required": true
      },
      {
        "id": "names",
        "type": "Symbol",
        "name": "Comma seperated Website name list (max 3)",
        "description": "Example: My Site",
        "required": true
      },
      {
        "id": "netlifySiteUrls",
        "type": "Symbol",
        "name": "Comma seperated Netlify site URLs (max 3)",
        "description": "Example: https://firefly-12345.netlify.com",
        "required": true
      }
    ]
  }
}
